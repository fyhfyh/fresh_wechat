/* pages/userlogin/index.wxss */
.register {
    background-image: -webkit-gradient(linear,left top,left bottom,from(#0cac54),to(#0cac54));
    background-image: linear-gradient(180deg,#44c58c 0,#0079029d);
    background-image: -moz-linear-gradient(to bottom,#0cac54 0,#0cac54 100%);
    width: 100%;
    height: 100vh;
}
.register .shading {
    background-image: "/images/registerw.png";
    background-repeat: no-repeat;
    background-size: 100% 100%;
    width: 100%;
    height: 16.86rem;
    padding-top: 10rem;
}

.register .shading .pictrue {
    width: 5.72rem;
    height: 5.72rem;
    border-radius: 50%;
    background-color: 
    hsla(0,0%,100%,.8);
    margin: 0 auto;
}
.register .shading .pictrue #img-logo {
    width: 5.54rem;
    height: 5.54rem;
    border-radius: 50%;
    display: block;
}
.list{
    width: 12rem;
    height: 3rem;

}
.register .whiteBg .list .item {
    border-bottom: 1px solid 
    #ededed;
    padding: .47rem 0 .13rem 0;
    position: relative;
}
.acea-row.row-center-wrapper {
    -webkit-box-align: center;
    -o-box-align: center;
    -ms-flex-align: center;
    align-items: center;
}

.acea-row {
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-lines: multiple;
    -moz-box-lines: multiple;
    -o-box-lines: multiple;
    -ms-flex-wrap: wrap;
    flex-wrap: wrap;
}
.register .whiteBg {
    width: 16.2rem;
    border-radius: .3rem;
    background-color: #fff;
    margin: .5rem auto 0 auto;
    padding: 1rem 15rem 10rem 3rem;

}
.register .whiteBg .list .item .icon {
    font-size: .35rem;
    margin-right: .32rem;
}
.icon {
    width: 1em;
    height: 1em;
    vertical-align: -.15em;
    fill: currentColor;
    overflow: hidden;
}
.input-login{

  height: 3rem;
}
.register .bottom {

    /* background-image:url(../../images/bootom.png); */
    background-repeat: no-repeat;
    background-size: 100% 100%;
    width: 18.2rem;
    height: 1rem;
    margin: 0 auto;

}

.register .whiteBg .logon {
font-size: 1rem;
color: #fff;
font-weight: 700;
height: 2rem;
border-radius: 1rem;
background: -webkit-gradient(linear,left top,right top,from(#0079029d),to(#0cac54a8));
text-align: center;
line-height: 2rem;
margin-top: 2rem;

}
